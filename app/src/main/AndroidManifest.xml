<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="brothersideas.mx.dragonproyect">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProyectsActivity" />
        <activity
            android:name=".MetasActivity"
            android:label="@string/title_activity_metas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HisotoriasActivity"
            android:label="@string/title_activity_hisotorias"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetalleHistoria"
            android:label="@string/title_activity_detalle_historia"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NuevoProyecto" />
        <activity android:name=".NuevaMeta" />
        <activity android:name=".NuevaHistoria" />
        <activity android:name=".ProyectosExtras"></activity>
    </application>

</manifest>